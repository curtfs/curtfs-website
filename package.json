{
  "name": "curt-website",
  "version": "0.1.0",
  "scripts": {
    "start": "ng serve --port=5200",
    "analyze": "ng build --prod --stats-json && webpack-bundle-analyzer dist/curt/stats-es2015.json",
    "fcm": "concat dist/curt/ngsw-worker.js src/messaging.js -o dist/curt/combined-worker.js",
    "deploy:staging": "npm run build && firebase use staging && npm run firebase:deploy",
    "deploy:prod": "npm run build && firebase use default && npm run firebase:deploy",
    "firebase:deploy": "firebase deploy --only hosting",
    "build": "ng build --prod && npm run fcm && cp src/messaging.js dist/curt/firebase-messaging-sw.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.1",
    "@angular/cdk": "~8.1.1",
    "@angular/common": "~8.0.1",
    "@angular/compiler": "~8.0.1",
    "@angular/core": "~11.0.5",
    "@angular/fire": "^5.2.1",
    "@angular/forms": "~8.0.1",
    "@angular/material": "^8.1.1",
    "@angular/platform-browser": "~8.0.1",
    "@angular/platform-browser-dynamic": "~8.0.1",
    "@angular/pwa": "^0.801.2",
    "@angular/router": "~8.0.1",
    "@angular/service-worker": "~8.0.1",
    "angulartics2": "^7.5.2",
    "firebase": "^6.3.1",
    "firebaseui": "^4.1.0",
    "firebaseui-angular": "^3.4.2",
    "hammerjs": "^2.0.8",
    "idb-keyval": "^3.2.0",
    "ng-in-viewport": "^6.0.3",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular/cli": "~8.0.3",
    "@angular/compiler-cli": "~8.0.1",
    "@angular/language-service": "~8.0.1",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "concat": "^1.0.3",
    "first-input-delay": "^0.1.3",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3",
    "webpack-bundle-analyzer": "^3.3.2"
  }
}
